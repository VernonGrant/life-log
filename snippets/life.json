{
    "date": {
        "prefix": "ld",
        "body": [
            "$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "$0"
        ],
        "description": "Insert date stamp"
    },
    "inbox": {
        "prefix": "li",
        "body": [
            "INBOX:",
            "$0"
        ],
        "description": "Insert inbox heading"
    },
    "task": {
        "prefix": "lt",
        "body": [
            "00:00 T #$0"
        ],
        "description": "Insert new task"
    },
    "tasktime": {
        "prefix": "ltt",
        "body": [
            "$CURRENT_HOUR:$CURRENT_MINUTE T #$0"
        ],
        "description": "Insert new task with current timestamp"
    }
}